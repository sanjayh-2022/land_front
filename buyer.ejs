<%layout('./layouts/boilerplate')%>
<body>
    <div style="height: 110px;">  </div>
    <section class="mainbuyer py-5" >
        <div class="container py-5">
         
             <span class="usercircle"><img src="data:image/jpeg;base64," alt=""></span>
    
        </div>
    </section>
    <section class="buyerprintingdetails">
        <h2 class="ms-5 mb-3">Buyer Details:</h2>
        <div class="row offset-1">
            
            <p class="card-text ml-3"><b>Name: </b><i><%= userObject.name %></i></p>
            <p class="card-text"><b>age: </b> <%= userObject.age %></p>
            <p class="card-text"><b>city: </b> <%= userObject.city %> </p>
            <p class="card-text"><b>email: </b><%= userObject.email %> </p>
            <p class="card-text"><b>Wallet Address: </b><%= userObject.id %></p>  
        </div>
    </section>
    <section style="overflow: hidden !important;position: relative;margin-top: 20rem;">
        <h3 class="ms-5 mb-5">Lands Listed:</h3>
        <div class="row row-cols-lg-3 ms-5">
            <% landDetailsArray.forEach(function(land) { %>
                <a href="/buyer/<%= land.id%>">
                    <div class="card mb-5">
                        <div class="img-wrapper">
                            <img src="https://media.istockphoto.com/id/909423306/photo/modern-multilevel-house-exterior-with-pool.webp?b=1&s=170667a&w=0&k=20&c=lVkUVRVlGBio7oMSYkx_tQuydlhJnBkpnVSuBBHtRMM="
                                alt="...">
                        </div>
                        <div class="card-body">
                            <h5 class="card-title head1" style="font-weight: 600;">Land <%= land.id %></h5>
                            <p class="card-text per1">Area: <%= land.area %> sq ft</p>
                            <p class="card-text per1">Price: &#8377; <%= land.landPrice %></p>
                        </div>
                    </div>
                </a>
                <% }); %>
    </div>
    </section>
    
</body>